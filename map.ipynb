   "source": [
    "# map is used to iterate iterable through a function but it returns map obj so we need to typecast it \n",
    "def cube(x):\n",
    "    return x*x\n",
    "l=[1,2,3,4,5]\n",
    "ll=list(map(cube,l))\n",
    "print(ll)"
   ]
